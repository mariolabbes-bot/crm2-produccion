{
  "name": "crm2-backend",
  "version": "2.0.2",
  "main": "index.js",
  "scripts": {
    "start": "node --no-warnings src/index.js",
    "dev": "npx kill-port 3001 || true && nodemon src/index.js",
    "worker": "node src/worker.js",
    "start:clean": "npx kill-port 3001 || true && node src/index.js",
    "seed": "node src/seed.js",
    "test": "jest --runInBand",
    "import:ventas": "node scripts/import_full_base_fast.js",
    "import:abonos": "node scripts/import_abonos_fast.js",
    "users:merge:dry": "DRY_RUN=true node scripts/merge_duplicate_users.js",
    "users:merge": "DRY_RUN=false node scripts/merge_duplicate_users.js",
    "import:model:dry": "DRY_RUN=true node scripts/import_model_excel.js",
    "import:model": "node scripts/import_model_excel.js"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "bull": "^4.10.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "fast-csv": "^5.0.5",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.12",
    "pg": "^8.11.1",
    "pg-copy-streams": "^7.0.0",
    "uuid": "^9.0.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "supertest": "^7.1.1"
  }
